<p id="notice"><%= notice %></p>
<% if current_user %>
<h1>User: <%= current_user.name%></h1>
<% else %>
<h1>Sign in to participate in events</h1>
<% end %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= check_usere(current_user, user) %></td>
        <td><%= check_userd(current_user, user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


